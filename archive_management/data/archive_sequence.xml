<odoo>

    <record id="seq_archive_storage" model="ir.sequence">
        <field name="name">Archive Storage sequence</field>
        <field name="code">archive.storage</field>
        <field name="prefix">AS</field>
        <field name="padding">8</field>
    </record>

    <record id="seq_archive_file" model="ir.sequence">
        <field name="name">Archive File sequence</field>
        <field name="code">archive.file</field>
        <field name="prefix">AF</field>
        <field name="padding">8</field>
    </record>

    <record id="seq_archive_location" model="ir.sequence">
        <field name="name">Archive Location sequence</field>
        <field name="code">archive.location</field>
        <field name="prefix">LC</field>
        <field name="padding">8</field>
    </record>

    <record id="seq_archive_multi_storage_transfer" model="ir.sequence">
        <field name="name">Archive Multi storage transfer sequence</field>
        <field name="code">archive.multi.storage.transfer</field>
        <field name="prefix">TR</field>
        <field name="padding">8</field>
    </record>

</odoo>
